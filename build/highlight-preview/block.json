{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "rmse-vat-results/highlight-preview",
  "version": "1.0.9",
  "title": "Highlight of the next game",
  "category": "club-api",
  "icon": "format-status",
  "description": "Show a highlight of the next game",
  "supports": {
    "align": true
  },
  "attributes": {
    "team": {
      "type": "string",
      "default": ""
    },
    "teamSource": {
      "type": "string",
      "default": "static"
    },
    "teamAcfField": {
      "type": "string",
      "default": ""
    },
    "venue": {
      "type": "boolean",
      "default": true
    },
    "dateformat": {
      "type": "string",
      "default": "d.m.y H:i"
    },
    "logos": {
      "type": "boolean",
      "default": true
    },
    "names": {
      "type": "boolean",
      "default": true
    },
    "logosize": {
      "type": "string",
      "default": "90"
    },
    "matchcenter": {
      "type": "boolean",
      "default": false
    },
    "layout": {
      "type": "string",
      "enum": [
        "legacy",
        "modern"
      ],
      "default": "legacy"
    }
  },
  "textdomain": "rmse-vat-results",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "render": "file:./render.php"
}