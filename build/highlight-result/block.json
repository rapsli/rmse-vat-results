{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "rmse-vat-results/highlight-result",
  "version": "1.0.8",
  "title": "Highlight of the last results",
  "category": "widgets",
  "icon": "format-status",
  "description": "Show a highlight of the last result",
  "supports": {
    "align": true
  },
  "attributes": {
    "team": {
      "type": "string",
      "default": ""
    },
    "venue": {
      "type": "boolean",
      "default": true
    },
    "dateformat": {
      "type": "string",
      "default": "d.m.y"
    },
    "logos": {
      "type": "boolean",
      "default": true
    },
    "names": {
      "type": "boolean",
      "default": true
    },
    "spectators": {
      "type": "boolean",
      "default": true
    },
    "halftime": {
      "type": "boolean",
      "default": true
    },
    "logosize": {
      "type": "string",
      "default": "90"
    }
  },
  "textdomain": "rmse-vat-results",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewScript": "file:./view.js",
  "render": "file:./render.php"
}
