{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "rmse-vat-results/rankings",
	"version": "1.0.9",
	"title": "Team Ranking",
	"category": "club-api",
	"icon": "index-card",
	"description": "Show Ranking of the team and the general information",
	"supports": {
		"align": true
	},
	"attributes": {
		"team": {
			"type": "string",
			"default": ""
		},
		"teamSource": { "type": "string", "default": "static" },
		"teamAcfField": { "type": "string", "default": "" },
		"header": {
			"type": "boolean",
			"default": true
		},
		"title": {
			"type": "boolean",
			"default": true
		},
		"logo": {
			"type": "boolean",
			"default": true
		},
		"logosize": {
			"type": "string",
			"default": "35"
		}
	},
	"textdomain": "rmse-vat-results",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"viewScript": "file:./view.js",
	"render": "file:./render.php"
}
