{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "rmse-vat-results/games",
	"version": "1.0.9",
	"title": "Club Games and Results",
	"category": "club-api",
	"icon": "list-view",
	"description": "Show the next games / last results of the club as a whole",
	"supports": {
		"align": true
	},
	"attributes": {
		"results": {
			"type": "integer",
			"default": 10
		},
		"scheduled": {
			"type": "integer",
			"default": 10
		},
		"header": {
			"type": "boolean",
			"default": true
		},
		"venue": {
			"type": "boolean",
			"default": true
		},
		"group": {
			"type": "boolean",
			"default": true
		},
		"with_result": {
			"type": "boolean",
			"default": true
		},
		"dateformat": {
			"type": "string",
			"default": "d.m.y H:i"
		}
	},
	"textdomain": "rmse-vat-results",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"viewScript": "file:./view.js",
	"render": "file:./render.php"
}
