{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "rmse-vat-results/schedule",
	"version": "1.0.9",
	"title": "Team Schedule",
	"category": "widgets",
	"icon": "calendar",
	"description": "Show the schedule and last results of a team",
	"supports": {
		"align": true
	},
	"attributes": {
		"results": {
			"type": "integer",
			"default": 10
		},
		"scheduled": {
			"type": "integer",
			"default": 10
		},
		"team": {
			"type": "string",
			"default": ""
		},
		"header": {
			"type": "boolean",
			"default": true
		},
		"venue": {
			"type": "boolean",
			"default": true
		},
		"group": {
			"type": "boolean",
			"default": true
		},
		"with_result": {
			"type": "boolean",
			"default": true
		},
		"dateformat": {
			"type": "string",
			"default": "d.m.y H:i"
		}
	},
	"textdomain": "rmse-vat-results",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"viewScript": "file:./view.js",
	"render": "file:./render.php"
}
